{
  "version": 2,
  "builds": [
    {
      "src": "ecom/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb",
        "includeFiles": ["staticfiles/**", "ecom/staticfiles/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ecom/wsgi.py"
    }
  ]
}